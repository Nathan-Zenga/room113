		<header>
			<div class="inner-header row">
				<div class="col-xs-3 menu-icon">
					<%_ if ( !url.includes('admin') ) { _%>
						<div class="hamburger hamburger--squeeze">
							<span class="hamburger-box">
								<span class="hamburger-inner"></span>
							</span>
						</div>
					<%_ } _%>
				</div>
				<div class="col-xs-6 logo"><a href="/"></a></div>
				<div class="col-xs-3"></div>
			</div>
			<%_ if ( !url.includes('admin') ) { _%>
				<div class="header-news-banner row">
					<div class="carousel slide" id="featured-spotlight">

						<!-- Container for slides -->
						<div class="carousel-inner">
							<%_ if (items) { _%>
								<%_ items.forEach((item, i) => { _%>
									<div class="item <%= i == 0 ? 'active' : '' %>">
										<div class="img"></div>
										<div class="carousel-caption">
											<h3 class="caption-headline"><%= item.headline %></h3>
											<!-- <div class="caption-text"></div> -->
										</div>
									</div>
								<%_ }) _%>
							<%_ } _%>
						</div>

						<!-- Left + right arrow controls -->
						<a class="left carousel-control" data-slide="prev">
							<span class="glyphicon glyphicon-chevron-left"></span>
							<span class="sr-only">Previous</span>
						</a>
						<a class="right carousel-control" data-slide="next">
							<span class="glyphicon glyphicon-chevron-right"></span>
							<span class="sr-only">Next</span>
						</a>
					</div>
				</div>
			<%_ } _%>
		</header>